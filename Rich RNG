-- Lib

local Mercury = loadstring(game:HttpGet("https://raw.githubusercontent.com/deeeity/mercury-lib/master/src.lua"))()

-- Gui

local GUI = Mercury:Create{
    Name = "YowaiHub",
    Size = UDim2.fromOffset(600, 400),
    Theme = Mercury.Themes.Dark,
    Link = "SubToScripter_Yowai"
}

-- MainTab
local AutoQuestWeeklyTab = GUI:Tab{
        Name = "QuestWeekly",
        Icon = "rbxassetid://8569322835"
}

-- AnotherTab
local AutoQuestDailyTab = GUI:Tab{
          Name = "QuestDaily",
          Icon = "rbxassetid://8569322835"
}


-- Toggel For WeeklyQuest
AutoQuestWeeklyTab:Toggle{
	Name = "Quest GODLY",
	StartingState = false,
	Description = "AutoQuest",
	Callback = function(state)
	autogodly = state
	while autogodly do


game:GetService("ReplicatedStorage").Remotes.Quest:FireServer("Claim","GODLY!")
   wait()
  end
end
}


-- Toggel For WeeklyQuest
AutoQuestWeeklyTab:Toggle{
	Name = "Quest Play V",
	StartingState = false,
	Description = "AutoQuest",
	Callback = function(state)
	autoplayv = state
	while autoplayv do


game:GetService("ReplicatedStorage").Remotes.Quest:FireServer("Claim","Play V")
   wait()
  end
end
}

